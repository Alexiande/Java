package ru.mirea.lab7;

public class TestMyArrayList {
    public static void main(String[] args) {
        MyArrayListApp<Integer> numbers = new MyArrayListApp<>(8);
        for(int i = 0; i<20;i++)
            numbers.add(i);
        System.out.println(numbers);
        for(int i = 5; i < 13; ++i)
            numbers.remove(i);
        System.out.println(numbers);
        System.out.println(numbers.get(5));
        System.out.println();

        MyArrayListApp<String> string = new MyArrayListApp<>();
        System.out.println(string);
        for (int i = 0; i < 10; i++) {
            string.add("String number " + (i+1));
        }
        System.out.println(string);
        System.out.println();

        MyArrayListApp<String> string2 = new MyArrayListApp<>(string);
        System.out.println(string2);
        string2.remove(5);
        System.out.println(string2.get(5));
        System.out.println(string.get(5));
        string2.addAll(string2.getCount(), string.subList(4, 8));
        System.out.println(string2);
        string2.sort(String::compareTo);
        System.out.println(string2);
    }
}
