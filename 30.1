package ru.mirea.task30;

public class Calculator {

    public int getSum(int a, int b) {
        return a + b;
    }

    public int getDif(int a, int b) {
        return a - b;
    }

    public int getMult(int a, int b) {
        return a * b;
    }

    public double getDiv(int a, int b) {
        return a / b;
    }

    public int getSquare(int c) {
        return c * c;
    }
}
